{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "<% if (orm === 'prisma') { %>npm run migrate:dev && <% } %>run-p dev:*",
    "dev:client": "cross-env NUXT_PORT=<%= clientPort %> nuxt-ts",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida<% if (orm === 'prisma') { %> && prisma generate --schema server/prisma/schema.prisma<% } %> && nuxt-ts <%= target === 'static' ? 'generate' : 'build' %>",
    "build:server": "npm run build --prefix server",
    "build:types": "aspida && npm run build:frourio --prefix server",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --ext .ts,.js,.vue .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:style -- --fix<% if (orm === 'prisma') { %>",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server<% } else if (orm === 'typeorm') { %>",
    "migration:generate": "npm run migration:generate --prefix server",
    "migration:run": "npm run migration:run --prefix server",
    "migration:revert": "npm run migration:revert --prefix server<% } %>",
    "start": "run-p start:*",
    "start:client": "nuxt-ts <%= target === 'static' ? 'serve' : 'start' %>",
    "start:server": "npm start --prefix server<% if (testing === 'jest') { %>",
    "test": "npm run build:types && jest<% } %>",
    "typecheck": "npm run build:types && tsc --noEmit && tsc --noEmit -p server"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "<% if (aspida === 'axios') { %>@aspida/axios": "^1.1.1",
    "<% } else { %>@aspida/fetch": "^1.1.1",
    "@aspida/node-fetch": "^1.1.1",
    "<% } %>@nuxt/typescript-runtime": "^2.0.0<% if (aspida === 'axios') { %>",
    "@nuxtjs/axios": "^5.12.4<% } %>",
    "class-validator": "^0.12.2",
    "core-js": "^3.8.1",
    "nuxt": "2.14.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/stylelint-module": "^4.0.0<% if (orm === 'prisma') { %>",
    "@prisma/cli": "^2.13.1<% } %><% if (testing === 'jest') { %>",
    "@types/jest": "^26.0.19<% } %><% if (aspida === 'fetch') { %>",
    "@types/node-fetch": "^2.5.7<% } %><% if (testing === 'jest') { %>",
    "@vue/test-utils": "^1.1.2",
    "babel-core": "7.0.0-bridge.0<% } %>",
    "babel-eslint": "^10.1.0<% if (testing === 'jest') { %>",
    "babel-jest": "^26.6.3<% } %>",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6<% if (testing === 'jest') { %>",
    "jest": "^26.6.3<% } %>",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0<% if (testing === 'jest') { %>",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "vue-jest": "^3.0.7<% } %>"
  }
}
